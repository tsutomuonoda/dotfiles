	    var elements3 = document.getElementById(\"gl\").options[document.getElementById(\"gl\").selectedIndex].value; 
	    var url1 = \"https://s3.amazonaws.com/JP_AM/weekly_top1000/history/\";
	    var url2 = \"_monthly/\"
	    var url3 = url1 + elements + elements2 + url2;
	    var url4 = \"Top1000_\"
	    var url5 = \".txt\"
	    var url6 = url3 + url4 + elements3 + url5;
		today=new Date();
		y=today.getFullYear();
		m=today.getMonth()+1;
		d=today.getDate();
	  if ( y > elements)  {
	  window.open(url6);
     }
         else if(elements2 >= m)  {
         alert(\"恐れ入りますが指定された期間のレポートはまだ作成されておりません。\");
         }
	         else if(d < 3)  {
	         alert(\"恐れ入りますが指定された期間のレポートはまだ作成されておりません。\");
	         }
   else {
    window.open(url6);
   }
	} 
// -->
</script> 


